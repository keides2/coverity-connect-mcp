# Coverity Connect MCP Server - 本番環境設定
# 実際のCoverity Connect サーバー向けの設定

# Coverityサーバー設定
COVERITY_HOST=your-coverity-server.company.com
COVERITY_PORT=8080
COVERITY_SSL=True

# 認証情報（実際の値に置き換えてください）
COVAUTHUSER=your_username
COVAUTHKEY=your_auth_key

# 本番環境設定
MCP_DEBUG=False
LOG_LEVEL=INFO

# セキュリティ設定
SSL_VERIFY=True
TIMEOUT_SECONDS=30

# 注意: 
# 1. 実際の認証情報に置き換えてください
# 2. このファイルは機密情報を含むため、安全に管理してください
# 3. .gitignore に含めて、バージョン管理から除外してください